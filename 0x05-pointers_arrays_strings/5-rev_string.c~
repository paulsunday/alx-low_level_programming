#include "main.h"
/**
* rev_string- Reversed string
* @s: parameters
* Return: Always Zero
*/
void rev_string(char *s)
{
char tmp;
int n, length1, length2;
length1 = 0;
length2 = 0;
while (s[length1] != '\0')
length1++;
length2 = length1 - 1;
for (n = 0; n < length1 / 2; n++)
{
tmp = s[n];
s[i] = s[length2];
s[length2] = tmp;
length2 -= 1;
}
}
