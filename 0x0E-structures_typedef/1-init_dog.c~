#include "dog.h"
/**
 * int_dog- init a var of type struct dog
 * @d: point to the dog
 * @name: name of dog
 * @age: age of the dog
 * @owner: of the dog
 * Return: void
 *
*/
void init_dog(struct dog *d, char *name, float age, char *owner)
{
if (d)
{
d->name = name;
d->age = age;
d->owner = owner;
}
}
